{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aws/aws-sdk-php": "^3.369.24",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.6.2",
        "geoip2/geoip2": "^3.3",
        "minishlink/web-push": "^10.0",
        "phpoffice/phpspreadsheet": "^5.4",
        "phpseclib/phpseclib": "^3.0.49",
        "predis/predis": "^3.3",
        "symfony/asset": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/flex": "^2.10",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/lock": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/messenger": "8.0.*",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/rate-limiter": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/scheduler": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/string": "8.0.*",
        "symfony/telegram-notifier": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/ux-autocomplete": "^2.32",
        "symfony/ux-chartjs": "^2.32",
        "symfony/ux-dropzone": "^2.32",
        "symfony/ux-icons": "^2.32",
        "symfony/ux-live-component": "^2.32",
        "symfony/ux-turbo": ">=2.32",
        "symfony/validator": "8.0.*",
        "symfony/webpack-encore-bundle": ">=2.4",
        "symfony/workflow": "8.0.*",
        "symfony/yaml": "8.0.*",
        "twig/cssinliner-extra": "^3.23",
        "twig/extra-bundle": "^3.23.0",
        "twig/inky-extra": "^3.23",
        "twig/string-extra": "^3.23",
        "twig/twig": "^3.23.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "giorgiosironi/eris": "^1.0",
        "phpunit/phpunit": "^11.5.50",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear --no-warmup": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "php vendor/bin/phpunit",
        "test:property": "php vendor/bin/phpunit --testsuite 'Property Tests'"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "8.0.*"
        }
    }
}
